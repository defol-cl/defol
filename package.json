    "clean": "pnpm recursive exec -- rm -rf node_modules; rm shrinkwrap.yaml; rm -rf node_modules",
    "server:start": "pnpm recursive run start --filter @beaver/server",
    "test": "echo \"Error: no test specified\" && exit 1",
